{
  "name": "cubex/sitemap",
  "authors": [
    {
      "name": "Kyle Essex",
      "email": "kylegavinessex@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "cubex/framework": "^4.17",
    "packaged/glimpse": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.9",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "auto",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Cubex\\Sitemap\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cubex\\Sitemap\\Tests\\": "tests"
    }
  }
}
